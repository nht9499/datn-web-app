{
  "name": "hbs-admin-portal",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite dev",
    "dev:stag": "vite dev --mode stag",
    "dev:prod": "vite dev --mode prod",
    "build": "npm run build:stag",
    "build:stag": "vite build --mode staging",
    "build:prod": "vite build --mode production",
    "check": "vue-tsc --noEmit",
    "deploy": "npm run run deploy:stag",
    "deploy:stag": "firebase deploy --only hosting --project kitor-stag",
    "deploy:prod": "firebase deploy --only hosting --project kitor",
    "all:stag": "run-s build:stag deploy:stag",
    "all:prod": "run-s build:prod deploy:prod",
    "start": "vite preview",
    "test:gzip": "npx http-server dist --cors --gzip -c-1",
    "test:br": "npx http-server dist --cors --brotli -c-1"
  },
  "dependencies": {
    "@any-touch/core": "^2.2.0",
    "@any-touch/pan": "^2.2.0",
    "@fawmi/vue-google-maps": "^0.9.72",
    "@quasar/extras": "^1.15.8",
    "@styled-icons/material-outlined": "^10.47.0",
    "@unocss/reset": "^0.47.5",
    "@vueuse/core": "^9.6.0",
    "agora-rtc-sdk-ng": "^4.14.0",
    "apexcharts": "^3.35.4",
    "axios": "^0.27.2",
    "dayjs": "^1.11.3",
    "firebase": "~9.8.3",
    "immer": "^9.0.15",
    "mitt": "^3.0.0",
    "object-hash": "^3.0.0",
    "pinia": "^2.0.22",
    "pinia-plugin-persistedstate": "^2.2.0",
    "quasar": "^2.10.2",
    "sortablejs": "^1.15.0",
    "static-google-map": "^0.0.4",
    "vue": "^3.2.45",
    "vue-currency-input": "^3.0.0",
    "vue-final-modal": "^3.4.4",
    "vue-i18n": "^9.1.10",
    "vue-router": "^4.1.5",
    "vue3-apexcharts": "^1.4.1",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^3.4.0",
    "@quasar/vite-plugin": "^1.2.3",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.6.3",
    "@types/object-hash": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.4.0",
    "npm-run-all": "^4.1.5",
    "sass": "1.32.12",
    "typescript": "^4.7.3",
    "unocss": "^0.47.5",
    "vite": "^3.2.5",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-pages": "^0.26.0",
    "vite-plugin-vue-layouts": "^0.7.0",
    "vite-svg-loader": "^3.6.0",
    "vue-tsc": "^1.0.12"
  }
}
